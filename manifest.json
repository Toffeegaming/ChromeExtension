{
  "name": "ZuydOp",
  "version": "1.0.0",
  "description": "Ga snel naar de juiste website voor het smartboard.",
  "manifest_version": 3,
  "author": "Cas olischl√§ger",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_title": "Zuyd MirrorOp",
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "/images/Icon_16.png",
      "32": "/images/Icon_32.png",
      "48": "/images/Icon_48.png",
      "128": "/images/Icon_128.png"
    }
  },
  "icons": {
    "16": "/images/Icon_16.png",
    "32": "/images/Icon_32.png",
    "48": "/images/Icon_48.png",
    "128": "/images/Icon_128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "zuydop@mozillafirefox",
      "strict_min_version": "109.0"
    }
  }
}
